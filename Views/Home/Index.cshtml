@{
    ViewBag.Title = "Home";
    <script>
        localStorage["rate"] = @ViewBag.Rate;
        localStorage["incentiveRate"] = @ViewBag.IncentiveRate;
        localStorage["premiumRate"] = @ViewBag.PremiumRate;
        localStorage["bonusRate"] = @ViewBag.BonusRate;
        localStorage["center"] = '@ViewBag.Center';
    </script>
}
@section SPAViews {
    <app-root></app-root>
    @Scripts.Render("~/SPA")
}